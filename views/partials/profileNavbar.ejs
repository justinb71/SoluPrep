<script>
    let menuOpen = false;

    function menuToggle(){
        const menuButton = document.querySelector(".nav-menu-button");
        const menu = document.querySelector(".nav-menu");
        if (menuOpen){
            menuButton.innerHTML = '<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none"> <path d="M3 12H21M3 6H21M3 18H21" stroke="#667085" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/> </svg>'
            menu.style.visibility = "hidden"
            window.scrollTo(0, 0);
            document.body.style.overflow = 'scroll';
            menuOpen = false;
        }else{
            menuButton.innerHTML = '<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none"> <path d="M18 6L6 18M6 6L18 18" stroke="#667085" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/> </svg>'
            menu.style.visibility = "visible"
            window.scrollTo(document.END)
            document.body.style.overflow = 'hidden';
            menuOpen = true;
        }

    }

    function initialize(){
        username = document.querySelector(".username").textContent;
        profileIcon = document.querySelector(".profile_icon")
        profileIcon.textContent = username[0]

    }
    initialize()

    var rippleEffect = (function(){
    var className, ripple;
    
    className = 'btns';
    ripple = document.createElement("div")
    ripple.classList.add('ripples')
    
    document.addEventListener('mousedown', function(e) {
        if (e.target.classList.contains(className)) {
        ripple.setAttribute("style", "top: " + e.offsetY + "px; left: " + e.offsetX + "px");
        e.target.appendChild(ripple)
        }
    })
    })();
</script>