<script>
    let menuOpen = false;

    function menuToggle(){
        const menuButton = document.querySelector(".nav-menu-button");
        const menu = document.querySelector(".nav-menu");
        if (menuOpen){
            menuButton.innerHTML = '<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none"> <path d="M3 12H21M3 6H21M3 18H21" stroke="#667085" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/> </svg>'
            menu.style.visibility = "hidden"
            window.scrollTo(0, 0);
            document.body.style.overflow = 'scroll';
            menuOpen = false;
        }else{
            menuButton.innerHTML = '<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none"> <path d="M18 6L6 18M6 6L18 18" stroke="#667085" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/> </svg>'
            menu.style.visibility = "visible"
            window.scrollTo(document.END)
            document.body.style.overflow = 'hidden';
            menuOpen = true;
        }

    }

    function initialize(){
        username = document.querySelector(".username").textContent;
        profileIcon = document.querySelector(".profile_icon")
        profileIcon.textContent = username[0]

    }
    initialize()

    var rippleEffect = (function(){
    var className, ripple;
    
    className = 'btns';
    ripple = document.createElement("div")
    ripple.classList.add('ripples')
    
    document.addEventListener('mousedown', function(e) {
        if (e.target.classList.contains(className)) {
        ripple.setAttribute("style", "top: " + e.offsetY + "px; left: " + e.offsetX + "px");
        e.target.appendChild(ripple)
        }
    })
    })();

document.addEventListener('DOMContentLoaded', function () {
    const checkbox = document.getElementById('checkbox');
    const navbarContents = document.querySelector('.authenticated_navbar');
    const line_wobble_frame = document.querySelector('.line-wobble-frame');
    const main = document.querySelector('.main');
    const hamburger = document.querySelector('#menuToggle');

    // Function to check the checkbox based on screen width
    function checkCheckboxByScreenWidth() {
        if (window.innerWidth < 1024) { // Adjust the screen width threshold as needed
            checkbox.checked = true;
            navbarContents.classList.add('hide-navbar');
            if (main) {
                main.style.left = '0px';
                main.style.width = '100%';
                hamburger.style.left = "99px";
                hamburger.style.top = "-33px";
                if (line_wobble_frame){
                        line_wobble_frame.style.width = '100%';
                        line_wobble_frame.style.left = '0px';
                    }
            }
        } else {
            const savedCheckboxState = localStorage.getItem('checkboxState');
            checkbox.checked = savedCheckboxState === 'checked';

            if (checkbox.checked) {
                navbarContents.classList.add('hide-navbar');
                if (main) {
                    main.style.left = '0px';
                    main.style.width = '100%';
                    if (line_wobble_frame){
                        line_wobble_frame.style.width = '100%';
                        line_wobble_frame.style.left = '0px';
                    }
                    
                    hamburger.style.left = "99px";
                    hamburger.style.top = "-33px";
                }
            } else {
                navbarContents.classList.remove('hide-navbar');
                if (main) {
                    main.style.left = '345px';
                    main.style.width = 'calc(100% - 345px)';
                    if (line_wobble_frame){
                        line_wobble_frame.style.width = 'calc(100% - 345px)';
                        line_wobble_frame.style.left = '345px';
                    }
                    
                    hamburger.style.left = "20px";
                    hamburger.style.top = "0px";
                }
            }
        }
    }

    // Check the checkbox on page load
    checkCheckboxByScreenWidth();

    // Attach an event listener to the checkbox
    checkbox.addEventListener('change', function () {
        if (this.checked) {
            navbarContents.classList.add('hide-navbar');
            if (main) {
                main.style.left = '0px';
                main.style.width = '100%';
                hamburger.style.left = "99px";
                hamburger.style.top = "-33px";
                if (line_wobble_frame){
                        line_wobble_frame.style.width = '100%';
                        line_wobble_frame.style.left = '0px';
                    }
            }
        } else {
            navbarContents.classList.remove('hide-navbar');
            if (main) {
                main.style.left = '345px';
                main.style.width = 'calc(100% - 345px)';
                hamburger.style.left = "20px";
                hamburger.style.top = "0px";
                if (line_wobble_frame){
                        line_wobble_frame.style.width = 'calc(100% - 345px)';
                        line_wobble_frame.style.left = '345px';
                    }
            }
        }

        // Save checkbox state to local storage
        localStorage.setItem('checkboxState', this.checked ? 'checked' : 'unchecked');
    });

    // Attach a resize event listener to update the checkbox on window resize
    window.addEventListener('resize', function () {
        checkCheckboxByScreenWidth();
    });
});



</script>

<style>
    #menuToggle{
        position: relative;
        z-index: 100;
        
        /* visibility: hidden; */
    }
    #checkbox {
    display: none;
    }

    .toggle {
    position: relative;
    width: 20px;
    cursor: pointer;
    margin: auto;
    display: block;
    height: calc(4px * 3 + 11px * 2);
    }

    .bar {
    position: absolute;
    left: 0;
    right: 0;
    height: 4px;
    border-radius: calc(4px / 2);
    background: #8b898d;
    color: inherit;
    opacity: 1;
    transition: none 0.35s cubic-bezier(.5,-0.35,.35,1.5) 0s;
    }

    /***** Collapse Animation *****/

    .bar--top {
    bottom: calc(25% + 11px + 4px/ 2);
    transition-property: bottom,margin,transform;
    transition-delay: calc(0s + 0.35s),0s,0s;
    }

    .bar--middle {
    top: calc(50% - 4px/ 2);
    transition-property: top,opacity;
    transition-duration: 0.35s,0s;
    transition-delay: calc(0s + 0.35s * 1.3),calc(0s + 0.35s * 1.3);
    }

    .bar--bottom {
    top: calc(25% + 11px + 4px/ 2);
    transition-property: top,transform;
    transition-delay: 0s;
    }

    #checkbox:checked + .toggle .bar--top {
    bottom: calc(50% - 11px - 4px);
    margin-bottom: calc(11px + 4px/ 2);
    transform: rotate(45deg);
    transition-delay: calc(0s + 0.35s * .3),calc(0s + 0.35s * 1.3),calc(0s + 0.35s * 1.3);
    }

    #checkbox:checked + .toggle .bar--middle {
    top: calc(50% + 11px);
    opacity: 0;
    transition-duration: 0.35s,0s;
    transition-delay: 0s,calc(0s + 0.35s);
    }

    #checkbox:checked + .toggle .bar--bottom {
    top: calc(50% - 4px/ 2);
    transform: rotate(-45deg);
    transition-delay: calc(0s + 0.35s * 1.3),calc(0s + 0.35s * 1.3);
    }

    .ui-switch {
  /* switch */
  --switch-bg: rgb(135, 150, 165);
  --switch-width: 38px;
  --switch-height: 15px;
  /* circle */
  --circle-diameter: 26px;
  --circle-bg: rgb(0, 56, 146);
  --circle-inset: calc((var(--circle-diameter) - var(--switch-height)) / 2);
}

.ui-switch input {
  display: none;
}

.slider {
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
  width: var(--switch-width);
  height: var(--switch-height);
  background: var(--switch-bg);
  border-radius: 999px;
  position: relative;
  cursor: pointer;
}

.slider .circle {
  top: calc(var(--circle-inset) * -1);
  left: 0;
  width: var(--circle-diameter);
  height: var(--circle-diameter);
  position: absolute;
  background: var(--circle-bg);
  border-radius: inherit;
  background-image: url("data:image/svg+xml;base64,");
  background-repeat: no-repeat;
  background-position: center center;
  -webkit-transition: left 150ms cubic-bezier(0.4, 0, 0.2, 1) 0ms, -webkit-transform 150ms cubic-bezier(0.4, 0, 0.2, 1) 0ms;
  -o-transition: left 150ms cubic-bezier(0.4, 0, 0.2, 1) 0ms, transform 150ms cubic-bezier(0.4, 0, 0.2, 1) 0ms;
  transition: left 150ms cubic-bezier(0.4, 0, 0.2, 1) 0ms, transform 150ms cubic-bezier(0.4, 0, 0.2, 1) 0ms, -webkit-transform 150ms cubic-bezier(0.4, 0, 0.2, 1) 0ms;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  box-shadow: 0px 2px 1px -1px rgba(0,0,0,0.2), 0px 1px 1px 0px rgba(0,0,0,0.14), 0px 1px 3px 0px rgba(0,0,0,0.12);
  ;
}

.slider .circle::before {
  content: "";
  position: absolute;
  width: 100%;
  height: 100%;
  background: rgba(255, 255, 255, 0.75);
  border-radius: inherit;
  -webkit-transition: all 500ms;
  -o-transition: all 500ms;
  transition: all 500ms;
  opacity: 0;
}

/* actions */

.ui-switch input:checked+.slider .circle {
  left: calc(100% - var(--circle-diameter));
  background-image: url("data:image/svg+xml;base64,");
}

.ui-switch input:active+.slider .circle::before {
  -webkit-transition: 0s;
  -o-transition: 0s;
  transition: 0s;
  opacity: 1;
  width: 0;
  height: 0;
}

</style>